#include "Std_Types.h"
#include "UART__Init.h"
#include "Dio_Int.h"
#include "Dio_Reg.h"
#include "Utilites.h"
#include "SPI_Init.h"


void main (void){


	UART_Init();
	u8 x = 0;
	Dio_SetPinDirection(GroupC, PIN7, OUTPUT);
	Dio_SetPinDirection(GroupC, PIN2, OUTPUT);

	while (1){


		x = UART_ReceiveData();

		UART_SendData(x);




	/*	if (x ==ö 'A') {

			Dio_SetPinValue(GroupC, PIN7, HIGH);
			SPI_MasterInit();
			SPI_SendData(x);

		}else if(x>0 && x != 'A'){

			Dio_SetPinValue(GroupC, PIN2, HIGH);
		}*/
	}

}
